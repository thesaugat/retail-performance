{
    "version": 2,
    "builds": [
        {
            "src": "api/matches.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/matches",
            "dest": "/api/matches.js"
        },
        {
            "src": "/(.*\\.(js|css|map|ico|svg))",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}